      "source": [
        "def sigmoid(z):\n",
        "  z=1/(1+np.exp(-z))\n",
        "  return z"
      ],
      "metadata": {
        "id": "nvkSEVsZ2AEg"
      },
      "execution_count": 40,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def compute_cost(X,y,w,b):\n",
        "  m=X.shape[0]\n",
        "\n",
        "  f_wb=sigmoid(np.dot(X,w)+b)\n",
        "  cost=-np.mean(y*np.log(f_wb)+(1-y)*np.log(1-f_wb))\n",
        "  return cost"
      ],
      "metadata": {
        "id": "Eb5C4N4m2pa9"
      },
      "execution_count": 41,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def reg_cost(X,y,w,b,lambda_):\n",
        "  total_cost=compute_cost(X,y,w,b)\n",
        "  m,n=X.shape\n",
        "  reg_cost=0\n",
        "  for i in range(n):\n",
        "    reg_cost+=w[i]**2\n",
        "  reg_cost*=lambda_/(2*m)\n",
        "  return total_cost+reg_cost"
      ],
      "metadata": {
        "id": "DRyyezVw6X97"
      },
      "execution_count": 42,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def compute_gradient(X,y,w,b):\n",
        "  m,n=X.shape\n",
        "  f_wb=sigmoid(np.dot(X,w)+b)\n",
        "  err=(f_wb-y).reshape(-1,1)\n",
        "  dj_db=np.mean(err)\n",
        "  dj_dw=np.mean((err)*X,axis=0)\n",
        "  return dj_dw,dj_db"
      ],
      "metadata": {
        "id": "NxTXLg2975wP"
      },
      "execution_count": 43,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def reg_gradient(X,y,w,b,lambda_):\n",
        "  m,n=X.shape\n",
        "  dj_dw,dj_db=compute_gradient(X,y,w,b)\n",
        "  dj_dw+=(lambda_*w)/m\n",
        "  return dj_dw,dj_db"
      ],
      "metadata": {
        "id": "tFa9pObLCMeq"
      },
      "execution_count": 44,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def gradient_descent(X, y, w_in, b_in, alpha, num_iters, lambda_):\n",
        "  m=X.shape[0]\n",
        "  J_history=[]\n",
        "  w_history=[]\n",
        "  for i in range(num_iters):\n",
        "    dj_dw,dj_db=reg_gradient(X,y,w_in,b_in,lambda_)\n",
        "    b_in=b_in-alpha*dj_db\n",
        "    w_in=w_in-alpha*dj_dw\n",
        "    if i<100000:\n",
        "      cost=reg_cost(X,y,w_in,b_in,lambda_)\n",
        "      J_history.append(cost)\n",
        "    if i% math.ceil(num_iters/10) == 0 or i == (num_iters-1):\n",
        "            w_history.append(w_in)\n",
        "            print(f\"Iteration {i:4}: Cost {float(J_history[-1]):8.2f}   \")\n",
        "\n",
        "  return w_in, b_in, J_history, w_history"
      ],
      "metadata": {
        "id": "sgURRnvFDZdI"
      },
      "execution_count": 51,
      "outputs": []
    },
