        "initial_w = np.zeros(X.shape[1]).reshape(-1)\n",
        "initial_b = 0.\n",
        "iterations = 10000\n",
        "alpha = 0.001\n",
        "w,b, J_history,_ = gradient_descent(X ,y, initial_w, initial_b, alpha, iterations, 0)\n"
